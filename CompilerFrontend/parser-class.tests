  <control> {
    <string:"class">
    <string:"CSimple">
    <control> {
      <string:"subroutine">
      <string:"test">
      <string:"public">
      <string:"instance">
      <list> {
        <list> {
          <string:"inArgA">
          <string:"()">
          <path> {
            <string:"Integer">
          }
        }
        <list> {
          <string:"inArgB">
          <null>
          <path> {
            <string:"Obj">
          }
        }
      }
      <list> {
        <statement> {
          <path> {
            <string:"MsgBox">
            <list> {
              <expression> {
                <string:"Hello World! ">
                <operator:add>
                <path> {
                  <string:"inArgB">
                }
              }
            }
          }
        }
      }
    }
  }
}

